    "@fontsource/roboto": "^5.0.3",
    "@formkit/auto-animate": "^0.7.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.13.7",
    "@mui/system": "^5.13.6",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^13.5.0",
    "animate.css": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "cloudinary-react": "^1.8.1",
    "formik": "^2.4.2",
    "gsap": "^3.12.2",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^27.5.1",
    "mongodb": "^5.6.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-awesome-reveal": "^4.2.5",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-material-ui-carousel": "^3.4.2",
    "react-number-format": "^5.2.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "react-tooltip": "^5.18.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "sass": "^1.58.3",
    "styled-components": "^6.0.4",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "swiper": "^10.0.4",
    "web-vitals": "^2.1.4",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-styles-sass start-js",
    "build": "npm run eslint && npm run stylelint-build && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "watch-styles-sass": "npm-watch",
    "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
    "eslint": "eslint src/**/*.js src/**/*.jsx"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js",
        "/src/**/*.jsx"
      ],
      "extensions": "js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "standard": "^10.0.3"
  }
}
